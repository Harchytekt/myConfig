"*":
  "advanced-open-file":
    createDirectories: true
    createFileInstantly: true
    defaultInputValue: "Project root"
  "atom-beautify":
    apex:
      disabled: true
    css:
      beautify_on_save: true
      end_with_newline: true
    general:
      beautifyEntireFileOnSave: false
    html:
      beautify_on_save: true
      end_with_newline: true
      indent_inner_html: true
    java: {}
    js:
      beautify_on_save: true
    json:
      beautify_on_save: true
    php:
      beautify_on_save: true
    sass:
      beautify_on_save: true
  "atom-clock":
    dateFormat: "H:mm, dddd D MMM"
    locale: "fr"
  "autocomplete-python":
    useKite: false
  autoprefixer:
    cascade: false
    runOnSave: true
  "battery-status":
    pollingInterval: 15
    showRemainingTime: false
  "bracket-matcher":
    highlightMatchingLineNumber: true
  "color-picker":
    uppercaseColorValues: true
  core:
    disabledPackages: [
      "spell-check"
      "atom-terminal-tab"
      "linter-markdown"
      "linter-csslint"
      "reindent"
      "linter"
      "atom-ide-ui"
      "ide-java"
      "autocomplete-python"
    ]
    packagesWithKeymapsDisabled: [
      "tool-bar"
    ]
    telemetryConsent: "no"
  editor:
    atomicSoftTabs: false
    fontFamily: "Fira Code"
    preferredLineLength: 120
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softTabs: false
    softWrap: true
    tabLength: 4
    tabType: "hard"
  "exception-reporting":
    userId: "b515fdac-fcab-4687-9bd9-7582f2abac6f"
  latex:
    engine: "xelatex"
  "linter-eslint":
    lintHtmlFiles: true
  "linter-htmlhint": {}
  "linter-jshint":
    lintInlineJavaScript: true
  "linter-sass-lint":
    globalSassLint: true
  "linter-ui-default":
    alwaysTakeMinimumSpace: true
    panelHeight: 146
    showProviderName: true
  "markdown-preview": {}
  "markdown-toc-auto":
    max: 5
  "markdown-writer": {}
  minimap:
    plugins:
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      linter: true
      linterDecorationsZIndex: 0
      pigments: true
      pigmentsDecorationsZIndex: 0
  "minimap-git-diff":
    useGutterDecoration: true
  "minimap-linter": {}
  "nord-atom-syntax":
    accessibility: {}
  "nord-atom-ui": {}
  "northem-dark-atom-ui": {}
  "one-dark-ui": {}
  organized:
    enableToolbarSupport: false
  "pdf-view":
    fitToWidthOnOpen: true
  "sass-autocompile":
    indentWidth: 4
  term3:
    fontSize: "12"
  "terminal-plus":
    core:
      mapTerminalsTo: "Folder"
    style:
      theme: "pro"
    toggles: {}
  "tool-bar":
    fullWidth: false
    visible: false
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode-plus":
    notifiedCoffeeScriptNoLongerSupportedToExtendVMP: true
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
	